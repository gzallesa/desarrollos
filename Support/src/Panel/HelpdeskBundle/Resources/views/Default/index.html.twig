{% extends "::helpdesk.html.twig" %}
{% block scripts %}
    
{% endblock %}
{% block body %}
    <!-- Top line begins -->
    <div id="top">
        <div class="wrapper">
            <a href="#" title="" class="logo"><img src="{{ asset('bundleHelpdesk') }}/images/logo.png" alt="" /></a>

            <!-- Right top nav -->
            <div class="topNav">
                <ul class="userNav">
                    <li><a href="#" title="" class="settings"></a></li>
                    <li><a href="{{ path('support_support_logout') }}" title="" class="logout"></a></li>
                    <li class="showTabletP"><a href="#" title="" class="sidebar"></a></li>
                </ul>
                <a title="" class="iButton"></a>
                <a title="" class="iTop"></a>
                <div class="topSearch">
                    <div class="topDropArrow"></div>
                    <form action="">
                        <input type="text" placeholder="search..." name="topSearch" />
                        <input type="submit" value="" />
                    </form>
                </div>
            </div>

            <!-- Responsive nav -->
            <ul class="altMenu">
                <li><a href="index.html" title="">Dashboard</a></li>
                <li><a href="ui.html" title="" class="exp" id="current">UI elements</a>
                    <ul>
                        <li><a href="ui.html">General elements</a></li>
                        <li><a href="ui_icons.html">Icons</a></li>
                        <li><a href="ui_buttons.html">Button sets</a></li>
                        <li><a href="ui_grid.html" class="active">Grid</a></li>
                        <li><a href="ui_custom.html">Custom elements</a></li>
                        <li><a href="ui_experimental.html">Experimental</a></li>
                    </ul>
                </li>
                <li><a href="forms.html" title="" class="exp">Forms stuff</a>
                    <ul>
                        <li><a href="forms.html">Inputs &amp; elements</a></li>
                        <li><a href="form_validation.html">Validation</a></li>
                        <li><a href="form_editor.html">File uploads &amp; editor</a></li>
                        <li><a href="form_wizards.html">Form wizards</a></li>
                    </ul>
                </li>
                <li><a href="messages.html" title="">Messages</a></li>
                <li><a href="statistics.html" title="">Statistics</a></li>
                <li><a href="tables.html" title="" class="exp">Tables</a>
                    <ul>
                        <li><a href="tables.html">Standard tables</a></li>
                        <li><a href="tables_dynamic.html">Dynamic tables</a></li>
                        <li><a href="tables_control.html">Tables with control</a></li>
                        <li><a href="tables_sortable.html">Sortable &amp; resizable</a></li>
                    </ul>
                </li>
                <li><a href="other_calendar.html" title="" class="exp">Other pages</a>
                    <ul>
                        <li><a href="other_calendar.html">Calendar</a></li>
                        <li><a href="other_gallery.html">Images gallery</a></li>
                        <li><a href="other_file_manager.html">File manager</a></li>
                        <li><a href="other_404.html">Sample error page</a></li>
                        <li><a href="other_typography.html">Typography</a></li>
                    </ul>
                </li>
            </ul>
            <div class="clear"></div>
        </div>
    </div>
    <!-- Top line ends -->


    <!-- Sidebar begins -->
    <div id="sidebar">
        <div class="mainNav">
            <div class="user">
                
                <ul class="leftUser">
                    <li><a href="#" title="" class="sProfile">My profile</a></li>
                    <li><a href="#" title="" class="sMessages">Messages</a></li>
                    <li><a href="#" title="" class="sSettings">Settings</a></li>
                    <li><a href="#" title="" class="sLogout">Logout</a></li>
                </ul>
            </div>

            <!-- Responsive nav -->
            <div class="altNav">
                <div class="userSearch">
                    <form action="">
                        <input type="text" placeholder="search..." name="userSearch" />
                        <input type="submit" value="" />
                    </form>
                </div>

                <!-- User nav -->
                <ul class="userNav">
                    <li><a href="#" title="" class="profile"></a></li>
                    <li><a href="#" title="" class="messages"></a></li>
                    <li><a href="#" title="" class="settings"></a></li>
                    <li><a href="#" title="" class="logout"></a></li>
                </ul>
            </div>

            <!-- Main nav -->
            <ul class="nav">
                <li><a href="#" title="" class="active"><img src="images/icons/mainnav/dashboard.png" alt="" /><span>Dashboard</span></a></li>

            </ul>
        </div>

        <!-- Secondary nav -->
        <div class="secNav">
            <div class="secWrapper">
                <div class="secTop">
                    <div class="balance">
                        {% set val="0" %}
                        {% set suma=0 %}
                        {% for item in bar %}
                            {% set val=val~","~item.total %}
                            {% set suma=suma+item.total %}
                        {% endfor %}   
                        <div class="balInfo">Helpdesk:<span>{{ "now"|date("d/m/Y") }}</span></div>
                        <div class="balAmount"><span class="balBars"><!--{{ val }}--></span><span>&#353;={{ (suma/bar|length)|round }}</span></div>
                    </div>
                    <a href="#" class="triangle-red"></a>
                </div>

                <!-- Tabs container -->
                <div id="tab-container" class="tab-container">
                    <ul class="iconsLine ic3 etabs">
                        <li><a href="#alt0" title=""><span class="icos-clock"></span></a></li>
                        <li><a href="#alt1" title=""><span class="icos-user"></span></a></li>
                        <li><a href="#alt2" title=""><span class="icos-archive"></span></a></li>
                    </ul>

                    <div class="divider"><span></span></div>

                    <div id="alt0">

                        <!-- Sidebar chart -->
                        <div class="numStats">
                            SOLICITUDES ABIERTAS
                            <ul>
                                <li><a href="#" title="">{{ totalDia[0].abierto }}</a><span>hoy</span></li>
                                <li><a href="#" title="">{{ totales[0].abierto }}</a><span>total</span></li>
                            </ul>
                            <div class="clear"></div>
                        </div>

                        <div class="divider"><span></span></div>

                        <!-- Sidebar user list -->
                        <ul class="userList">
                            {% for item in abiertas %}
                                <li>
                                    <a href="#" title="">
                                        {% if item.foto %}
                                            <img style="width: 25px;height: 30px" src="https://siemi.oopp.gob.bo/doccargados/rrhh/{{ item.ci }}/FOTOS/{{ item.foto }}" alt="" />
                                        {% else %}
                                            <img style="width: 25px;height: 30px" src="{{ asset('bundleHelpdesk') }}/images/icons/usual/icon-user.png" alt="" />
                                        {% endif %}    
                                            
                                        <span class="contactName">
                                            {% if item.dias==0 %}
                                                {% if item.horas==0 %}
                                                    <strong style="font-size:9px"><span>hace({{ item.minutos }} minutos)</span></strong>
                                                {% else %}
                                                    <strong style="font-size:9px"><span>hace({{ item.horas }}h:{{  item.minutos }}m)</span></strong>    
                                                {% endif %}
                                            {% else %}
                                                <strong style="font-size:9px"><span>hace({{ item.dias }} dias, {{ item.horas }}h:{{  item.minutos }}m)</span></strong>    
                                            {% endif %}
                                            <i>{{ item.problema }}</i>
                                        </span>
                                        {% if item.dias==0 %}
                                                {% if item.horas==0 %}
                                                    <span class="status_available"></span>
                                                {% else %}
                                                    <span class="status_away"></span>
                                                {% endif %}
                                            {% else %}
                                                <span class="status_off"></span>
                                            {% endif %}
                                        <span class="clear"></span>
                                        <span class="time">{{ item.usuario }}...</span>
                                    </a>
                                </li>
                            {% endfor %}    
                        </ul>
                    </div>

                    <div id="alt1">

                        <!-- Sidebar chart -->
                        <div class="numStats">
                            SOLICITUDES HOY
                            <ul>
                                <li><a href="#" title="">{{ totalDia[0].total }}</a><span>total</span></li>
                                <li><a href="#" title="">{{ totalDia[0].abierto }}</a><span>abierto</span></li>
                                <li class="last"><a href="#" title="">{{ totalDia[0].cerrado }}</a><span>cerrado</span></li>
                            </ul>
                            <div class="clear"></div>
                        </div>

                        <div class="divider"><span></span></div>

                        <!-- Sidebar user list -->
                        <ul class="userList">
                            {% for item in por_soporte %}
                                <li>
                                    <a href="#" title="">
                                        {% if item.foto %}
                                            <img style="width: 25px;height: 30px" src="https://siemi.oopp.gob.bo/doccargados/rrhh/{{ item.ci }}/FOTOS/{{ item.foto }}" alt="" />
                                        {% else %}
                                            <img style="width: 25px;height: 30px" src="{{ asset('bundleHelpdesk') }}/images/icons/usual/icon-user.png" alt="" />
                                        {% endif %} 
                                        
                                        <span class="contactName">
                                            <strong style="font-size:9px">{{ item.name }}...<span>({{ item.total }}-{{ totalDia[0].total }})</span></strong>
                                            <i>{{ item.cargo }}</i>
                                        </span>
                                        {% if item.total==totalDia[0].cerrado %}
                                            <span class="status_available"></span>
                                        {% elseif item.total>totalDia[0].cerrado/2 %}
                                            <span class="status_off"></span>
                                        {% else %}
                                            <span class="status_away"></span>
                                        {% endif %}    
                                        <span class="clear"></span>
                                        <div class="contentProgress mt8"><div id="bar2" class="barB tipS" style="width: {{ (item.total*100/totalDia[0].total)|round }}%;" original-title="{{ (item.total*100/totalDia[0].total)|round }}%"></div></div>
                                    </a>
                                </li>
                            {% endfor %}    
                        </ul>
                        <div class="divider"><span></span></div>
                        <ul class="userList">
                            {% for item in rep_problema_fecha %}
                                <li>
                                    <a href="#" title="">
                                        <span class="contactName">
                                            <strong style="font-size:9px">{{ item.nombre }}...<span>({{ item.total }})</span></strong>
                                            
                                        </span>

                                        <span class="clear"></span>
                                        <div class="contentProgress mt8"><div id="bar2" class="barO tipS" style="width: {{ item.total*100/totalDia[0].total}}%;" original-title="{{ item.total*100/totalDia[0].total}}%"></div></div>
                                    </a>
                                </li>
                            {% endfor %}    
                        </ul>
                    </div>


                    <div id="alt2">

                        <!-- Sidebar chart -->
                        <div class="numStats">
                            RESUMEN DE SOLICITUDES
                            <ul>
                                <li><a href="#" title="">{{ totales[0].total }}</a><span>total</span></li>
                                <li><a href="#" title="">{{ totales[0].abierto }}</a><span>abierto</span></li>
                                <li class="last"><a href="#" title="">{{ totales[0].cerrado }}</a><span>cerrado</span></li>
                            </ul>
                            <div class="clear"></div>
                        </div>

                        <div class="divider"><span></span></div>

                        <!-- Sidebar user list -->
                        <ul class="userList">
                            {% for item in reporte %}
                                <li>
                                    <a href="#" title="">
                                        {% if item.foto %}
                                            <img style="width: 25px;height: 30px" src="https://siemi.oopp.gob.bo/doccargados/rrhh/{{ item.ci }}/FOTOS/{{ item.foto }}" alt="" />
                                        {% else %}
                                            <img style="width: 25px;height: 30px" src="{{ asset('bundleHelpdesk') }}/images/icons/usual/icon-user.png" alt="" />
                                        {% endif %}
                                        
                                        <span class="contactName">
                                            <strong style="font-size:9px">{{ item.name }}...<span>({{ item.total }}-{{ totales[0].total }})</span></strong>
                                            <i>{{ item.cargo }}</i>
                                        </span>
                                        {% if item.total==totales[0].cerrado %}
                                            <span class="status_available"></span>
                                        {% elseif item.total>totales[0].cerrado/2 %}
                                            <span class="status_off"></span>
                                        {% else %}
                                            <span class="status_away"></span>
                                        {% endif %}    
                                        <span class="clear"></span>
                                        <div class="contentProgress mt8"><div id="bar2" class="barB tipS" style="width: {{ (item.total*100/totales[0].total)|round }}%;" original-title="{{ (item.total*100/totales[0].total)|round }}%"></div></div>
                                    </a>
                                </li>
                            {% endfor %}    
                        </ul>
                        <div class="divider"><span></span></div>
                        <ul class="userList">
                            {% for item in rep_problema %}
                                <li>
                                    <a href="#" title="">
                                        <span class="contactName">
                                            <strong style="font-size:9px">{{ item.nombre }}...<span>({{ item.total }})</span></strong>
                                        </span>
                                        <span class="clear"></span>
                                        <div class="contentProgress mt8"><div id="bar2" class="barG tipS" style="width: {{ item.total*100/totales[0].total}}%;" original-title="{{ item.total*100/totales[0].total}}%"></div></div>
                                    </a>
                                </li>
                            {% endfor %}    
                        </ul>
                    </div>
                </div>

                <div class="divider"><span></span></div>

            </div> 
            <div class="clear"></div>
        </div>
    </div>
    <!-- Sidebar ends -->


    <!-- Content begins -->
    <div id="content">
        <div class="contentTop">
            <span class="pageTitle"><span class="icon-screen"></span>Dashboard</span>
            <ul class="quickStats">
                <li>
                    <a href="" class="blueImg"><img src="{{ asset('bundleHelpdesk') }}/images/icons/light/icon-vbar.png" alt="" /></a>
                    <div class="floatR"><strong class="blue">{{ totales[0].total }}</strong><span>total</span></div>
                </li>
                <li>
                    <a href="" class="redImg"><img src="{{ asset('bundleHelpdesk') }}/images/icons/light/icon-alert.png" alt="" /></a>
                    <div class="floatR"><strong class="blue">{{ totales[0].abierto }}</strong><span>abiertas</span></div>
                </li>
                <li>
                    <a href="" class="greenImg"><img src="{{ asset('bundleHelpdesk') }}/images/icons/light/icon-check.png" alt="" /></a>
                    <div class="floatR"><strong class="blue">{{ totales[0].cerrado }}</strong><span>cerradas</span></div>
                </li>
            </ul>
            <div class="clear"></div>
        </div>

        <!-- Breadcrumbs line -->
        <div class="breadLine">
            <div class="bc">
                <ul id="breadcrumbs" class="breadcrumbs">
                    <li class="current"><a href="#">Dashboard</a></li>
                </ul>
            </div>

        </div>

        <!-- Main content -->
        <div class="wrapper">
            <!-- 6 + 6 -->
            <div class="fluid">

                <!-- Messages #1 -->
                <div class="widget grid6">
                    <div class="whead">
                        <h6>SOLICITUDES DE HOY</h6>

                        <div class="clear"></div>
                    </div>

                    <ul class="messagesOne">
                        {% for solicitud in solicitudes %}
                            {% if solicitud.estado=="solucionado" %}
                                <li class="by_user">
                                    <a href="#" title="">
                                        {% if solicitud.foto %}
                                            <img style="width: 45px;height: 50px;" src="https://siemi.oopp.gob.bo/doccargados/rrhh/{{ solicitud.ci }}/FOTOS/{{ solicitud.foto }}" alt="" />
                                        {% else %}
                                            <img style="width: 25px;height: 30px" src="{{ asset('bundleHelpdesk') }}/images/icons/usual/icon-user.png" alt="" />
                                        {% endif %}
                                        
                                    </a>
                                    <div class="messageArea">
                                        <span class="aro"></span>
                                        <div class="infoRow">
                                            <span class="name"><strong>{{ solicitud.usuario }}</strong> {{ solicitud.cargo }}:</span>
                                            <span class="time">fecha solicitud: {{ solicitud.fecha_solicitud }} a hrs. {{ solicitud.hora_solicitud }}</span>
                                            <div class="clear"></div>
                                        </div>
                                        Solicita soporte de:{{ solicitud.problema }}
                                    </div>
                                    <div class="clear"></div>
                                </li>
                                <li class="by_me">
                                    <a href="#" title="">
                                        {% if solicitud.foto %}
                                            <img style="width: 45px;height: 50px;" src="https://siemi.oopp.gob.bo/doccargados/rrhh/{{ solicitud.cisop }}/FOTOS/{{ solicitud.fotosop }}" alt="" />
                                        {% else %}
                                            <img style="width: 25px;height: 30px" src="{{ asset('bundleHelpdesk') }}/images/icons/usual/icon-user.png" alt="" />
                                        {% endif %}

                                    </a>
                                    <div class="messageArea">
                                        <span class="aro"></span>
                                        <div class="infoRow">
                                            <span class="name"><strong>{{ solicitud.soporte }}</strong> solucionado:</span>
                                                    {% if solicitud.dias==0 %}
                                                        {% if solicitud.horas==0 %}
                                                    <span class="time">Tardó {{ solicitud.minutos }} minutos en dar solucion</span>
                                                {% else %}
                                                    <span class="time">Tardó {{ solicitud.horas }} hrs. {{ solicitud.minutos }} minutos en dar solucion</span>    
                                                {% endif %}
                                            {% else %}
                                                <span class="time">Tardó {{ solicitud.dias }} dias, {{ solicitud.horas }} hrs. {{ solicitud.minutos }} minutos en dar solucion</span>
                                            {% endif %}
                                            <div class="clear"></div>
                                        </div>
                                    </div>
                                    <div class="clear"></div>
                                </li>
                                <li class="divider"><span></span></li>
                                    {% else %}    
                                <li class="by_user">
                                    <a href="#" title="">
                                        {% if solicitud.foto %}
                                            <img style="width: 45px;height: 50px;" src="https://siemi.oopp.gob.bo/doccargados/rrhh/{{ solicitud.ci }}/FOTOS/{{ solicitud.foto }}" alt="" />
                                        {% else %}
                                            <img style="width: 25px;height: 30px" src="{{ asset('bundleHelpdesk') }}/images/icons/usual/icon-user.png" alt="" />
                                        {% endif %}
                                        
                                    </a>
                                    <div class="messageArea" style=" background-color:rgba(250,0,0,.03);">
                                        <span class="aro"></span>
                                        <div class="infoRow">
                                            <span class="name"><strong>{{ solicitud.usuario }}</strong> {{ solicitud.cargo }}:</span>
                                                    {% if solicitud.dias==0 %}
                                                        {% if solicitud.horas==0 %}
                                                    <span class="time">hace {{ solicitud.minutos }} minutos</span>
                                                {% else %}
                                                    <span class="time">hace {{ solicitud.horas }} hrs. {{ solicitud.minutos }} minutos</span>    
                                                {% endif %}
                                            {% else %}
                                                <span class="time">hace {{ solicitud.dias }} dias, {{ solicitud.horas }} hrs. {{ solicitud.minutos }} minutos</span>
                                            {% endif %}
                                            <div class="clear"></div>
                                        </div>
                                        Solicita soporte de:{{ solicitud.problema }}
                                    </div>
                                    <div class="clear"></div>
                                </li>
                            {% endif %}        


                        {% endfor %}


                    </ul>
                </div>

                <!-- Table -->
                <div class="widget grid6">
                    <div class="whead"><h6>SOLICITUDES ABIERTAS</h6><div class="clear"></div></div>
                    <div id="dyn" class="hiddenpars">
                        <a class="tOptions" title="Options"><img src="{{ asset('bundleHelpdesk') }}/images/icons/options" alt="" /></a>
                        <table cellpadding="0" cellspacing="0" border="0" class="dTable" id="tabla_abiertas">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>USUARIO<span class="sorting" style="display: block;"></span></th>
                                    <th>_PISO__</th>
                                    <th>PROBLEMA</th>
                                    <th>FECHA SOLICITUD</th>
                                    <th>TIEMPO TRANSCURRIDO</th>
                                </tr>
                            </thead>
                            <tbody>
                                {% for item in abiertas %}
                                    {% set piso = item.ip|split('.') %}
                                    <tr class="gradeX">
                                        <td>{{ item.id }}</td>
                                        <td>{{ item.usuario }}<br><cite style="font-size: xx-small">{{ item.cargo }}</cite></td>
                                        <td>PISO <b>{{ piso[2] }}</b></td>
                                        <td>{{ item.problema }}</td>
                                        <td class="center">{{ item.fecha_solicitud|date("d/m/Y") }}</td>
                                        {% if item.dias==0 %}
                                            {% if item.horas==0 %}
                                                <td class="center">{{ item.minutos }}m</td>
                                            {% else %}
                                                <td class="center">{{ item.horas }}h:{{ item.minutos }}m</td>
                                            {% endif %}
                                        {% else %}
                                            <td class="center">{{ item.dias }}dias {{ item.horas }}h:{{ item.minutos }}m</td>
                                        {% endif %}

                                    </tr>
                                {% endfor %} 
                            </tbody>    
                        </table> 
                    </div>
                </div>    
                <div class="clear"></div>
            </div>

        </div>
        <!-- Main content ends -->

    </div>
    <!-- Content ends -->
    <div id="demo"></div>

{% endblock %}
