            </div>
        </div>
        <ul>
            {% set c="" %}
            {% set ids="" %}
            {% for problema in problemas %}
                {% set c=0 %}
                {% for solicitud in solicitudes %}
                    {% if solicitud.idp==problema.idp %}
                        {% set c=solicitud.n %}
                        {% set ids=solicitud.id %}
                    {% endif %}
                {% endfor %}
                {% if c>0 %}
                    <li>
                        <div class="element">
                            <div class="indicador">{{ c }}</div>
                            <div class="botones">
                                {% if problema.fun!=null %}
                                    <div class="foto2">
                                        <img style="height:45px;width:45px;border-radius:50%;"
                                             src='https://siemi.oopp.gob.bo/doccargados/rrhh/{{ problema.fun.ci }}/FOTOS/{{ problema.fun.foto }}'>

                                    </div>
                                {% else %}
                                    <div class="foto2">


                                    </div>
                                {% endif %}
                                <div title="Solicitar soporte nuevamente"
                                     onclick="enviar({{ problema.idp }},
                                             '{{ problema.fun.name }}',
                                             '{{ problema.fun.email }}',
                                             '{{ usuario.name }}',
                                             '{{ problema.nombre }}'
                                             );"
                                     class="sol"><i class="fa fa-user-md fa-2x"></i></div>
                                <div title="Finalizar solicitud" onclick="finaliza({{ ids }});" class="cer"><i
                                            class="fa fa-thumbs-o-up fa-2x"></i></div>
                            </div>
                            <div class="icono">
                                <i class="fa {{ problema.icon }} fa-2x"></i>
                            </div>
                            <div class="tituloxx">{{ problema.nombre }}</div>
                        </div>
                    </li>
                {% else %}
